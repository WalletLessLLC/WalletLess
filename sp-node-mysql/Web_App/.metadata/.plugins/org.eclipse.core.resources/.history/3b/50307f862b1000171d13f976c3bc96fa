$("#messageForm").submit(function(event) {
	event.preventDefault();
	console.log("Submitting...");
	console.log("Username: " + document.getElementById("username").value);
	console.log("Subject: " + document.getElementById("subject").value);
	console.log("Body: " + document.getElementById("messageBody").value);
	console.log("Date: " + (new Date()));
	$.post(
		'http://155.246.138.85:3000/messages',
		[
			{
				message_id: 1,
				sender_id: 123,
				receiver_id: document.getElementById("username").value,
				subject: document.getElementById("subject").value,
				body: document.getElementById("messageBody").value,
				create_date: "03/23/2017",
				is_read: 0
			}
		],
		function(data, status) {
			console.log(data);
		}
	);
});
