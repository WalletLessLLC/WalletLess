var usersApp = angular.module('usersApp', []);
usersApp.controller('UsersListCrtl', function($scope, $http) {
	$http.get('http://155.246.138.62:3000/users').success(function(data) {
		$http.get('http://155.246.138.62:3000/user_info/anotherEmail').success(function(fullData) {
			$scope.userData = fullData
		})
	})
})